extends layout

block content
  .container
    h1.mt-5.mb-3 #{title}
    p.lead Welcome to my Bootstrap-styled webpage.
  
    .dashboard-table
      table.table.table-hover
        thead.table-dark
          tr
            th(scope="col") #
            th(scope="col") First
            th(scope="col") Last
            th(scope="col") Handle
        tbody
          tr
            th(scope="row") 1
            td Mark
            td Otto
            td @mdo
          tr
            th(scope="row") 2
            td Jacob
            td Thornton
            td @fat
          tr
            th(scope="row") 3
            td(colspan="2") Larry the Bird
            td @twitter
